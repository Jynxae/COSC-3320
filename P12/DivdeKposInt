from typing import List

def Divide(input, posInt):
    Visit = []
    input.sort()
    if len(input) % posInt != 0: #check to see if input can be divided
        return False
    counter = 0
    while input:
        # Get the first element in the sorted array and remove it from nums.
        start_num = input[counter]
        Visit.append(start_num)
        
        # Create a sub_array starting from start_num + 1.
        if counter == 0:
            sub_array = input[:posInt//2] #[start_num + 1]
            counter = counter + 1
        else:
            sub_array = input[posInt*counter: posInt*(counter+1)]
            counter = counter + 1

        if not helper(Visit, sub_array):
            return False
    return True

input1 = [1, 2, 2, 3]
posInt1 = 4
result = Divide(input1,posInt1)
print(result)
'''
Input:
1 2 3 3 4 5 5 6
4

Output:
false

Input:
1 2 2 3
2

Output:
true


Input:
1 2 3 4 5
2

Output:
false
'''